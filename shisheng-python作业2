>>> a=10
>>> b=21
>>> a+b
31
>>> a-b
-11
>>> a*b
210
>>> a/b
0.47619047619047616
>>> b/a
2.1
>>> b%a
1
>>> a**b
1000000000000000000000
>>> a//b
0
>>> b//a
2

>>> a=10
>>> b=20
>>> a==b
False
>>> a!=b
True
>>> a>b
False
>>> a<b
True
>>> a>=b
False
>>> a<=b
True

>>> c=a+b
>>> c
30
>>> c+=a
>>> c
40
>>> c-=a
>>> c
30
>>> c*=a
>>> c
300
>>> c/=a
>>> c
30.0
>>> c%=a
>>> c
